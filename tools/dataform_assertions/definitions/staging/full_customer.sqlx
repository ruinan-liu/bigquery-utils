
config {
    type: "table",
    schema: "dev_data_zone",
    name: "full_customer_information",
    assertions: {
        uniqueKey: ["customerID"],
        nonNull: ["first_name","last_name","date_of_birth","gender","RFC", "telephone_number"]
    }
}

SELECT
    customerID,
    first_name,
    last_name,
    second_last_name,
    marital_status,
    date_of_birth,
    gender,
    RFC,
    telephone_number,
    email
FROM
    ${ ref("customer_info") }
LEFT JOIN
    ${ ref("customer") }
USING
    (customerID)